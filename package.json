    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/ws": "^8.2.0",
    "@vercel/node": "^1.12.1",
    "prisma": "^3.4.0",
